
sum of even and odd numbers
#include <stdio.h>

int main(void) 
{
	int a,even=0,odd=0,i;
	printf("enter the number of digits\n");
	scanf("%d",&a);
	for(i=1;i<=a;i++)
	{
		if(i%2==0)
		even=even+i;
		else
		odd=odd+i;
	}
	printf("the sum of even numbers is %d\n",even);
	printf("the sum of odd numbers is %d\n",odd);

}


SWAPPING OF NUMBERS
#include <stdio.h>

int main(void) 
{
	int a,b,temp=0;
	printf("enter the values of a and b\n");
	scanf("%d %d",&a,&b);
	temp=a;
	a=b;
	b=temp;
	printf("the swapped value of a and b are %d and %d\n",a,b);
	

}


swapping without third variable
#include <stdio.h>

int main(void) 
{
	int a,b;
	printf("enter the values of a and b\n");
	scanf("%d %d",&a,&b);
	a=a+b;
	b=a-b;
	a=a-b;
	printf("the swapped value of a and b are %d and %d\n",a,b);
	

}
